package com.Ejercicios_Clase6;

import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertNotEquals;

import org.junit.Test;

public class claculadoraTest {	

	@Test
	public void prueba1() {	//1
		
		double n1=80.0f;
		double n2=3.0f;
		double valEsp=240.0;		
		assertEquals(valEsp,calculadora.multiplicar(n1, n2),0.0f);		//Se agrega tercer argumento DELTA.
	}
	
	@Test
	public void prueba2() {
		double n1=150.0f;
		double n2=80.0f;
		double n3=3.0;
		double valEsp=110.0f;		
		assertEquals(valEsp,calculadora.dividir(calculadora.sumar(n1, n2), n3),0.5f);		
		
	}
	
	

}
