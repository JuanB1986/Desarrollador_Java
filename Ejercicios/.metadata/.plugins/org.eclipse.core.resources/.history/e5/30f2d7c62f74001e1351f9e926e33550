    	objAlumno[0].setLegajo("J-6955");
    	objAlumno[0].setMateriasAprobadas(null);
    	listaAlumnos.add(objAlumno[0]);    
    	
    	tempAprobadas2.add("Informatica 1");
    	tempAprobadas2.add("Analisis Matematico 1");
    	
    	objAlumno[1] = new Alumno();
    	objAlumno[1].setNombre("Vanesa Sosa");
    	objAlumno[1].setLegajo("V-1589");
    	objAlumno[1].setMateriasAprobadas(tempAprobadas2);
    	listaAlumnos.add(objAlumno[1]);
    	
    	tempAprobadas3.add("Fisica 1");
    	tempAprobadas3.add("Analisis Matematico 1");
    	
    	objAlumno[2] = new Alumno();
    	objAlumno[2].setNombre("Lucia Perez");
    	objAlumno[2].setLegajo("L-1157");
    	objAlumno[2].setMateriasAprobadas(tempAprobadas3);
    	listaAlumnos.add(objAlumno[2]);
    	
    	
    	List<String> correlativasFisica1 = new ArrayList<String>();
    	List<String> correlativasFisica2 = new ArrayList<String>();
    	List<String> correlativasFisica3 = new ArrayList<String>();
    	List<String> correlativasAnalisis1 = new ArrayList<String>();
    	List<String> correlativasAnalisis2 = new ArrayList<String>();
    	List<String> correlativasInformatica1 = new ArrayList<String>();
    	
    	
