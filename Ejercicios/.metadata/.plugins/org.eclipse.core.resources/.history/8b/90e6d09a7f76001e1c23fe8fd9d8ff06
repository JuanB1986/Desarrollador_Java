package com.Ejercicios_Clase11;

import java.util.ArrayList;
import java.util.List;

import lombok.AccessLevel;
import lombok.Getter;
import lombok.Setter;

public class Persona {
	
	@Getter @Setter 
	(AccessLevel.PRIVATE) List<String> nombreApellido;
	
	@Getter @Setter 
	(AccessLevel.PRIVATE) List<String> fechaNacimiento;
	
	
	public Persona() {
		this.nombreApellido=new ArrayList<String>();
		this.fechaNacimiento=new ArrayList<String>();
	}
	 
	public String getNombre(int index) {
		return this.nombreApellido.get(index);
	}
	
	public void setNombre(String nombreApellido) {
		this.nombreApellido.add(nombreApellido);
	}
	
	public String getFechaNacimeinto(int index) {
		return this.fechaNacimiento.get(index);
	}
	
	public void setFechaNacimiento(String fechaNacimiento) {
		this.fechaNacimiento.add(fechaNacimiento);
	}
	
}
