package paquete;
import java.util.Scanner;


public class ejemplo {

	
	public static void main(String[] args) {
		
		Scanner lee_teclado=new Scanner (System.in);
		
		
		double ingresos;
		int superficie;
		int consumo;
		
		System.out.println("Ingresos: \n");	
		ingresos = lee_teclado.nextDouble();
	
		System.out.println("Superficie; \n");
		lee_teclado.nextInt();
		
		System.out.println("Consumo: \n");
		lee_teclado.nextInt();
		
		
		if (ingresos  < 748382.0f && superfie < 30 && consumo < 3330) 
		{
			System.out.println("\n Cat. A \n");			
		}
		
		
		
		
		
	}

}
