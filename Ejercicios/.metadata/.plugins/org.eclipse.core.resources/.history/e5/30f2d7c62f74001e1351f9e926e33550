    	Materia Analisis2 = new Materia();
    	Materia Fisica1 = new Materia();
    	Materia Fisica2 = new Materia();
    	Materia Fisica3 = new Materia();
    	Materia Informatica1 = new Materia();
    	
    	//Agrego objetos materia a lista de materias
