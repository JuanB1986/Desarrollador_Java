package Ejercicios;

public class Ejercicio1A {

	public static void main(String[] args) {
		int numero[]=new int [3];
		char letra='a';
		int i;
		
		
		numero[0]=100;
		numero[1]=80;
		numero[2]=60;
		
		String strNumero = numero.toString();		

		String strArray[]=strNumero.split("");
		
		for (i=0;i<numero.length;++i)
		{
			System.out.println(strArray[i]);
		}
		
	}

}
