package Ejercicios;
import java.io.IOException;
import java.nio.file.*;
import java.nio.file.Paths;

public class Ejercicio2 {

	public static void main(String[] args) throws IOException {
		
		Files archivo = null;
		int i;
		String filePath="C:\\Users\\JM\\Desktop\\e.txt";
		String file2Path= "./archivos/numeros.n";
		String fileLine="";
		
		int k=0;
	
		do {
			fileLine = archivo.readString(Paths.get(file2Path));
			System.out.print(fileLine.charAt(3));	
			//k=Integer.parseInt(fileLine.replaceAll("\n|\r", ""));
			System.out.print(k);	
		
		}while (fileLine.isBlank());
		
			
		
		
	}

}
