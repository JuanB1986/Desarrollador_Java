package Ejercicios_Clase1;


public class Ej1 {

	public static void main(String[] args) {
		int numInicio=5;
		int numFin=14;
		int contador=0;
		
		
		// Apartado A)
		
		contador=numInicio;
	/*	while ( contador <= numFin) 
		{
			System.out.println(contador);			
			++contador;
		}*/
		
		// Apartado B)
		
		contador=numInicio;
		while ( contador <= numFin) 
		{
			if ( (contador % 2)==0 )
				System.out.println(contador);			
			
			++contador;
		}
		
	}

}
